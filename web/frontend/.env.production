# BroBro Frontend - Production Environment
# These variables are loaded by Vite during production builds
# Access them via import.meta.env.VITE_*

# Backend API URL for production
# Update this to your production backend URL
VITE_API_URL=https://api.brobro.com

# WebSocket URL for collaboration features
VITE_WEBSOCKET_URL=wss://api.brobro.com/ws/collaborate

# Logging level: info, warn, error (disable debug logging)
VITE_LOG_LEVEL=info

# Disable development features
VITE_DEBUG=false
