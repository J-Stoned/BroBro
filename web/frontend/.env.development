# BroBro Frontend - Development Environment
# These variables are loaded by Vite during development
# Access them via import.meta.env.VITE_*

# Backend API URL for development
VITE_API_URL=http://localhost:8000

# WebSocket URL for collaboration features
VITE_WEBSOCKET_URL=ws://localhost:8000/ws/collaborate

# Logging level: debug, info, warn, error
VITE_LOG_LEVEL=debug

# Enable development features
VITE_DEBUG=true
